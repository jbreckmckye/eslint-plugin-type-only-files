{
  "name": "eslint-plugin-type-only-files",
  "version": "1.0.4",
  "author": "Jimmy Breck-McKye",
  "description": "ESLint rule for type-only TS files",
  "license": "MIT",
  "repository": "https://github.com/jbreckmckye/eslint-plugin-type-only-files",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "scripts": {
    "dev": "rm -rf dist || true && tsc --watch",
    "build": "rm -rf dist || true && tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/types": "5.10.2",
    "@typescript-eslint/utils": "5.10.2"
  },
  "devDependencies": {
    "typescript": "5.2.2"
  }
}
